{
	"name": "htmltagwrap",
	"version": "0.0.1",
	"private": false,
	"description": "Wraps selected code with HTML tags",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandHTMLTagWrap#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandHTMLTagWrap/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandHTMLTagWrap.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üõ¨‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/src/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"test:LinuxAndOSX": "node ./node_modules/vscode/bin/test",
		"typings": "typings"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.htmlTagWrap",
				"title": "Wrap in HTML Tags"
			}
		],
		"keybindings": [
			{
				"command": "extension.htmlTagWrap",
				"key": "alt+w",
				"mac": "alt+w",
				"when": "editorTextFocus"
			}
		]
	},
	"activationEvents": [
		"onCommand:extension.htmlTagWrap"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"chai": "5.1.0",
		"fs-extra": "11.2.0",
		"typings": "1.3.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "images/htmlTagWrap_icon.svg",
	"galleryBanner": {
		"color": "#596877",
		"theme": "dark"
	}
}
